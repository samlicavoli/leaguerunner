{
  "name": "leaguerunner",
  "version": "0.1.0",
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "bootstrap": "^5.2.3",
    "firebase": "^9.17.1",
    "flow-bin": "^0.200.0",
    "lint-staged": "^13.1.2",
    "prettier": "^2.8.4",
